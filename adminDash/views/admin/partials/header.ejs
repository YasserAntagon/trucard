<style>
  .main-header .sidebar-brand {
    float: left;
    background-color: transparent;
    background-image: none;
    padding: 15px 15px;
    font-family: fontAwesome;
  }
  .skin-black .main-header .logo {
    background-color: #fff;
    color: #000;
    border-bottom: 1px solid #f9f0f0;
    border-right: none;
  }
  .sidebarback {
    display: block;
  }
</style>
<header class="main-header">
  <a href="<%- linkurl.home  %>" id="dashboard" class="logo">
    <span class="logo-mini"><b>T</b></span>
    <span class="logo-lg"
      ><img src="images/logo/logo.png" height="30px"
    /></span>
  </a>
  <nav class="navbar navbar-static-top">
    <a
      href="javascript:;"
      class="sidebar-toggle"
      data-toggle="push-menu"
      role="button"
    >
      <span class="sr-only">Toggle navigation</span>
    </a>
    <a
      class="sidebar-brand sidebarback"
      href="javascript:void(0);"
      onclick="goBack()"
    >
      <button class="btn-pinterest btn btn-sm">
        <span class="btn-gradient radius-btn">
          <i class="fa fa-arrow-left"></i>
        </span>
        <span class="btn-text">BACK</span>
      </button>
    </a>
    <div class="navbar-custom-menu">
      <ul class="nav navbar-nav">
        <li class="dropdown user user-menu">
          <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown">
            <% if (aData.image =="0") { %>
            <img src="images/user.png" class="user-image profpic" />
            <%} else { %>
            <img
              src="<%-config.aUrl %><%- aData.image  %>"
              class="user-image profpic"
            />
            <% } %>
            <span class="hidden-xs"
              ><span class="profnameAdmin"
                ><%- aData.fName %> <%- aData.lName %></span
              ></span
            >
          </a>
          <ul class="dropdown-menu">
            <li class="user-header">
              <% if (aData.image =="0") { %>
              <img src="images/user.png" class="user-image profpic" />
              <%} else { %>
              <img
                src="<%-config.aUrl %><%- aData.image  %>"
                class="user-image profpic"
              />
              <% } %>
              <p>
                <span class="profname"></span> <br />
                <small><span class="email"><%- aData.email %></span></small>
                <small><%- aData.truID %></small>
              </p>
            </li>
            <li class="user-footer">
              <div class="pull-left">
                <a
                  href="<%- linkurl.profile  %>"
                  id="profileDetails"
                  class="btn btn-default btn-flat"
                  >Profile</a
                >
              </div>
              <div class="pull-right">
                <a href="/" class="btn btn-default btn-flat">Sign out</a>
              </div>
            </li>
          </ul>
        </li>
      </ul>
    </div>
  </nav>
</header>
<aside class="main-sidebar">
  <section class="sidebar">
    <!-- Sidebar user panel -->
    <div class="user-panel">
      <div class="pull-left image">
        <% if (aData.image =="0") { %>
        <img src="images/user.png" class="img-circle user-image profpic" />
        <%} else { %>
        <img
          src="<%-config.aUrl %><%- aData.image  %>"
          class="img-circle user-image profpic"
        />
        <% } %>
      </div>
      <div class="pull-left info">
        <p>
          <span class="profname"><%- aData.fName %> <%- aData.lName %></span>
        </p>
        <a href="javascript:;"
          ><i class="fa fa-circle text-success"></i> Online</a
        >
      </div>
    </div>
    <% if (aData.department =="development" || aData.isSuperAdmin) { %> 
