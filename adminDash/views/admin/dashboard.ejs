    <script id="top5Report" type="text/template">
      {{#partnerRevenue}}
      <tr>            
        <td style="width: 50%">
          {{companyName}}
        </td>
        <td style="width: 20%"> 
          <span class="fa fa-circle-o-notch fa-spin spinnerRev text-info"></span>
          <span class="spinnerVal hidden"><i class="fa fa-inr"></i> {{revenue}}</span>
        </td>
        <td style="width: 30%">
          <span class="fa fa-circle-o-notch fa-spin spinnerRev text-info"></span>
          <span class="spinnerVal hidden"><i class="fa fa-inr"></i> {{totalAmount}}</span>
        </td> 
  </tr>
      {{/partnerRevenue}}
    </script>
    <script id="top5consumerReport" type="text/template">
      {{#top5Txn}}
      <tr>            
        <td style="width: 50%">
          {{companyName}}
        </td>
        <td style="width: 20%">
          <span class="fa fa-circle-o-notch fa-spin spinnerDRev text-info"></span>
          <span class="spinnerDVal hidden">{{type}}<br /><small>{{truCoin}}</small></span>
        </td>
        <td style="width: 30%">
          <span class="fa fa-circle-o-notch fa-spin spinnerDRev text-info"></span>
          {{#isGold}}
          <span class="spinnerDVal hidden">{{qty}} gms</span>
          {{/isGold}} 
          {{^isGold}}
          <span class="spinnerDVal hidden"><i class="fa fa-inr"></i> {{totalAmount}}</span>
          {{/isGold}} 
        </td>
  </tr>
      {{/top5Txn}}
    </script>
    <script id="top5ActiveTxn" type="text/template">
      {{#top5Txn}}  
                  <tr>
                    <td>
                      {{#isPartner}}
                      <a target="_blank" href="/openPartnerDash?id={{truID}}"  title="Go to partner panel" class="text text-primary">{{name}}</a><br />{{truID}}
                      {{/isPartner}}
                      {{^isPartner}}
                      <a target="_blank" href="/openConsumerDash?id={{truID}}"  title="Go to consumer panel" class="text text-primary">{{name}}</a><br />{{truID}}
                      {{/isPartner}} 
                    <td>{{totalCount}}</td>
                    <td>{{totalAmount}}</td> 
                    <td>
                      <a 
                      href="javascript:;"
                      data-toggle="collapse"
                      data-target="#demo{{truID}}"
                      class="accordion-toggle"
                      aria-expanded="false" aria-controls="#demo{{truID}}">
                        <span class="text text-primary fa fa-2x fa-file-text-o"></span>
                    </a>
                    </td>
                  </tr>
      
                  <tr>
                    <td colspan="12" class="hiddenRow">
                      <div class="accordian-body collapse" id="demo{{truID}}">
                        <table class="table table-striped">
                          <thead>
                            <tr class="info"> 
                              <th>Consumption</th>
                              <th>TruCoin Gold<br /> in gms</th>
                              <th>TruCoin Silver<br /> in gms</th>
                              <th>Total Amount<br /> in INR</th>
                            </tr>
                          </thead>
                          <tbody>
                            {{#txnArr}}
                            <tr> 
                              <td>{{type}}<br />Total Txn Count : {{count}}</td>
                              <td>{{g24k}}</td>
                              <td>{{s99p}}</td>
                              <td>{{total}}</td>
                            </tr>{{/txnArr}}
                          </tbody>
                        </table>
                      </div>
                    </td>
                  </tr>
      {{/top5Txn}}
    </script>
</head>

