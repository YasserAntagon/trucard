<!DOCTYPE html>
<html>

<head>
    <%- include ('./partials/head'); %>
    <link href="css/cuprofile.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="truComponents/bootstrap/dist/css/bootstrap-editable.css">
    <!-- Main content -->
<style>
    .content {
    min-height: 250px;
    padding:1px;
        padding-right: 1px;
        padding-left: 1px;
    margin-right: 1px;
    margin-left: 1px;
    padding-left: 1px;
    padding-right: 1px;
}
</style>
<body class="skin-blue fixed sidebar-mini sidebar-collapse">
    <div class="wrapper">
        <%- include ('./partials/header'); %>

        <div class="content-wrapper">
            <section class="content">
                <div class="row">
                    <div class="col-md-12">

                        <!-- Profile Image -->
                        <div class="box box-primary">
                            <div class="box-body box-profile">
                                <div class="box-header with-border">
                                    <h3 class="box-title">User Profile</h3>
                                    <div class="box-tools pull-right">
                                        <a href="javascript:void(0)" 
                                            title="update your profile" id="btnedit" class="fa fa-pencil btn"></a>
                                    </div>
                                </div>
                                <section class="profile">
                                    <!-- title row -->


                                    <img class="profile-user-img img-responsive img-circle" data-src="<%-config.aUrl %>" src="images/user.png"
                                        alt="User profile picture">

                                    <h3 class="profile-username text-center"></h3>
                                    <p class="text-muted text-center designation">Software Engineer</p>

                              <!--       <div class="box box-primary">
                                        <div class="box-header with-border">
                                            <h3 class="box-title">About Me</h3>

                                        </div> 
                                        <div class="box-body"> -->
                                           <!--  <table class="table table-striped">
                                                <tbody>
                                                    <tr>
                                                        <td> -->
                                                            <div class="content">
                                                                <div class="cards">
                                                                    <input type="hidden" id="empid" />
                                                                    <div class="badgescard">
                                                                        <table class="table table-striped" id="user">
                                                                            <thead>
                                                                                <tr>
                                                                                    <th colspan="4">
                                                                                        Personal Information :
                                                                                    </th>

                                                                                </tr>
                                                                            </thead>
                                                                            <tbody>
                                                                                <tr>
                                                                                    <td>Employee Code :</td>
                                                                                    <td>
                                                                                        <a href="javascript:;" id="inline-username"
                                                                                            class="fname editable editable-click editable-disabled"
                                                                                            data-type="text" data-pk="1"
                                                                                            data-title="Enter username"></a>
                                                                                    </td>
                                                                                </tr>

                                                                                <tr>
                                                                                    <td>Mobile No :</td>
                                                                                    <td>
                                                                                        <a href="javascript:;" id="inline-mobile"
                                                                                            class="mobile editable editable-click editable-disabled"
                                                                                            data-type="text" data-pk="1"
                                                                                            data-placement="right"
                                                                                            data-placeholder="Required"
                                                                                            data-title="Enter your mobile no"></a>
                                                                                    </td>
                                                                                </tr>
                                                                                <tr>
                                                                                    <td>Contact No :</td>
                                                                                    <td>
                                                                                        <a href="javascript:;" id="telephone"
                                                                                            class="mobile editable editable-click editable-disabled"
                                                                                            data-type="text" data-pk="1"
                                                                                            data-placement="right"
                                                                                            data-placeholder="Required"
                                                                                            data-title="Enter your mobile no"></a>
                                                                                    </td>
                                                                                </tr>

                                                                                <tr>
                                                                                    <td>Email Id :</td>
                                                                                    <td>
                                                                                        <a href="javascript:;" id="inline-email"
                                                                                            class="email editable editable-click editable-disabled"
                                                                                            data-type="text" data-pk="1"
                                                                                            data-title="Enter email id"></a>
                                                                                    </td>
                                                                                </tr>
                                                                                <tr>
                                                                                    <td>Gender :</td>
                                                                                    <td>
                                                                                        <a href="javascript:;" id="sex"
                                                                                            data-type="select"
                                                                                            data-pk="1" data-value="1"
                                                                                            data-title="Select sex"
                                                                                            class="editable editable-click editable-disabled"></a>
                                                                                    </td>
                                                                                </tr>
                                                                                <tr>
                                                                                    <td>Date Of Birth :</td>
                                                                                    <td>
                                                                                        <a href="javascript:;" id="dob"
                                                                                            data-type="combodate"
                                                                                            data-value=""
                                                                                            data-format="YYYY-MM-DD"
                                                                                            data-viewformat="DD/MM/YYYY"
                                                                                            data-template="D / MMM / YYYY"
                                                                                            data-pk="1"
                                                                                            data-title="Select Date of birth"
                                                                                            class="editable editable-click editable-disabled"></a>
                                                                                    </td>
                                                                                </tr>

                                                                                <tr>
                                                                                    <td>Contact Address :</td>
                                                                                    <td>
                                                                                        <a href="javascript:;" id="cAddress"
                                                                                            class="editable editable-click editable-disabled"
                                                                                            data-type="address"
                                                                                            data-pk="1"
                                                                                            data-title="Please, fill address"></a>
                                                                                    </td>
                                                                                </tr>

                                                                                <tr>
                                                                                    <td>Registered Address :</td>
                                                                                    <td>
                                                                                        <a href="javascript:;" id="rAddress"
                                                                                            class="editable editable-click editable-disabled"
                                                                                            data-type="address"
                                                                                            data-pk="1"
                                                                                            data-title="Please, fill address"></a>
                                                                                    </td>
                                                                                </tr>

                                                                                <tr>
                                                                                    <td>Joining Date :</td>
                                                                                    <td>
                                                                                        <a href="javascript:;" id="jDate"
                                                                                            class="editable editable-click editable-disabled"></a>
                                                                                    </td>
                                                                                </tr>
                                                                                <tr>
                                                                                    <td>Skill Set :</td>
                                                                                    <td>
                                                                                        <a href="javascript:;" id="skillset"
                                                                                            class="editable editable-click editable-disabled"></a>
                                                                                    </td>
                                                                                </tr>


                                                                            </tbody>
                                                                        </table>
                                                                    </div>
                                                                </div>
                                                            </div>
