<!DOCTYPE html>
<html>
<head>
<%- include ('./partials/head'); %>
<link rel="stylesheet" href="css/upload.css" /> 
<%- include ('./partials/lheader'); %>
<div class="row content">
  <div class="col-lg-12">
        <div class="box box-widget box-widgets" id="existaddr">
            <div id="ConsumerKYCLoader" class="loaderContainer" style="display:none;">
                <div class="loader"></div>
            </div>
            <div class="box-header">
                <h3 class="box-title"> <i class="fa fa-file-text-o"></i> KYC Document
                </h3>
                <span class="pull-right">
                   KYC Activation : <button type="button" class="btn btn-info btn-sm" id="kycDate"></button>
                </span>
                <input type="hidden" id="cdnURL" value="<%- config.entityDoc  %>" />
                <hr>
            </div>
            <div class="box-body">
                <div class="col-sm-12">
                    <div class="container-fuild">
                      <div class="row">
                        <div class="moreupload"></div>
                        <div class="col-sm-6 dockychide hidden">
                          <div class="container-fuild">
                            <div class="row">
                              <div class="col-lg-12">
                                <div class="row">
                                  <div class="col-12">
                                    <h5 class="docheader">National ID</h5>
                                    <p class="small text-danger">
                                      KYC document should match one of the billing
                                      / permanent address
                                    </p>
                                  </div>
  
                                  <div class="col-xs-6">
                                    <div class="form-group">
                                      <!-- <div class="input-group"> -->
                                      <select
                                        class="form-control subdmitId"
                                        id="cmbBox"
                                        type="text"
                                        name="submitId[]"
                                        required=""
                                        placeholder=""
                                      >
                                        <option value="0">Select Document</option>
                                        <option value="AadhaarCard"
                                          >Aadhaar Card</option
                                        >
                                      </select>
                                      <!--  </div>
                                                                            <p class="text-danger" id="error_cmbBox"></p> -->
                                    </div>
                                  </div>
  
                                  <div class="col-xs-6">
                                    <div class="form-group">
                                      <input
                                        class="form-control kycdocnum number"
                                        id="docNumber"
                                        type="tel"
                                        name="kycdocnum[]"
                                        maxlength="12"
                                        required=""
                                        placeholder="Enter your Aadhaar number"
                                      />
                                    </div>
                                  </div>
  
                                  <div class="col-sm-12">
                                    <div class="row aadhaar">
                                      <div class="col-sm-6">
                                        <div class="uploadWrapper">
                                          <form
                                            class="imageUploadForm front"
                                            id="imageUploadForm"
                                          >
                                            <span
                                              class="helpText"
                                              id="helpText"
                                            ></span>
                                            <input
                                              class="uploadButton kycdoc"
                                              id="docFrontFile"
                                              type="file"
                                              accept="image/*"
                                              data-type="aadhaarfront"
                                            />
                                            <div
                                              class="uploadedImg"
                                              id="uploadedImg"
                                            ></div>
                                          </form>
                                          <p class="small text-center">
                                            Aadhaar front side.
                                          </p>
                                        </div>
                                      </div>
                                      <div class="col-sm-6">
                                        <div class="uploadWrapper">
                                          <form
                                            class="imageUploadForm back"
                                            id="imageUploadForm1"
                                          >
                                            <span
                                              class="helpText"
                                              id="helpText1"
                                            ></span>
                                            <input
                                              class="uploadButton kycdoc"
                                              id="docBackFile"
                                              type="file"
                                              name="files[]"
                                              accept="image/*"
                                              data-type="aadhaarback"
                                            />
                                            <div
                                              class="uploadedImg"
                                              id="uploadedImg1"
                                            ></div>
                                          </form>
                                          <p class="small text-center">
                                            Aadhaar back side.
                                          </p>
                                        </div>
                                      </div>
                                    </div>
                                    <!-- <div class="row hidden otherdoc"> 
                                                                            <div class="col-sm-6">
                                                                                <div class="uploadWrapper">
                                                                                    <form class="imageUploadForm front" id="imageUploadForm"><span class="helpText" id="helpText"></span>
                                                                                        <input class="uploadButton kycdoc" id="file" type="file" accept="image/*" data-type="aadhaarfront" />
                                                                                        <div class="uploadedImg" id="uploadedImg"></div>
                                                                                    </form>
                                                                                    <p class="small text-center">Upload Doc </p>
                                                                                </div>
                                                                            </div>
                                                                        </div> -->
                                  </div>
                                </div>
                                <div class="row">
                                  <div class="col-sm-12">
                                    <div class="dockychide hidden">
                                      <input
                                        type="button"
                                        value="Upload"
                                        class="btn btn-danger btn-sm"
                                        id="uploadKYCDoc"
                                        onclick="uploadKYCDoc(this);"
                                      />
                                    </div>
                                  </div>
                                </div>
                              </div>
                            </div>
                          </div>
                        </div>
                        <div class="col-sm-6 pandockychide hidden">
                          <div class="row pan">
                            <div class="col-12">
                              <h5 class="docheader">
                                Tax ID&nbsp;<small>(Optional)</small>
                              </h5>
                              <p class="small text-danger">
                                * For transactions above Rs. 50,000 in cash will
                                require pan card
                              </p>
                            </div>
                            <div class="col-xs-6">
                              <div class="form-group">
                                <select
                                  class="form-control pansubdmitId"
                                  id="cmbpanCard"
                                  type="text"
                                  required=""
                                  disabled=""
                                  name="submitId[]"
                                  placeholder=""
                                >
                                  <option value="PANCard">PAN Card</option>
                                </select>
                              </div>
                            </div>
                            <div class="col-xs-6">
                              <div class="form-group">
                                <input
                                  class="form-control"
                                  id="docNumber1"
                                  type="text"
                                  name="kycdocpan[]"
                                  maxlength="10"
                                  required=""
                                  placeholder="Enter your PAN number"
                                />
                              </div>
                              <p class="text-danger" id="error_docNumber1"></p>
                            </div>
                          </div>
                          <div class="row">
                            <div class="col-sm-8">
                              <div class="uploadWrapper">
                                <form
                                  class="imageUploadForm pan"
                                  id="imageUploadForm2"
                                >
                                  <span class="helpText" id="helpText2"></span>
                                  <input
                                    class="uploadButton kycdocpan"
                                    id="kycdocFile1"
                                    type="file"
                                    name="files[]"
                                    accept="image/*"
                                    data-type="pan"
                                  />
                                  <div
                                    class="uploadedImg"
                                    id="uploadedImg2"
                                  ></div>
                                </form>
                                <p class="small text-center">
                                  PAN front side
                                </p>
                              </div>
                            </div>
                          </div>
                          <div class="row">
                            <div class="col-sm-4">
                              <div class="pull-right pandockychide hidden">
                                <input
                                  type="button"
                                  value="Upload"
                                  class="btn btn-danger btn-sm"
                                  id="uploadKYCPAN"
                                  onclick="uploadKYCPAN(this);"
                                />
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
            </div>
        </div>
    </div> 
    <div class="modal fade" id="imagemodal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">
            <span aria-hidden="true">&times;</span
            ><span class="sr-only">Close</span>
          </button>
          <h4 class="modal-title" id="myModalLabel">Document Preview</h4>
        </div>
        <div class="modal-body">
          <img src="" id="docviewer" style="width: 100%; height: 380px;" />
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">
            Close
          </button>
        </div>
      </div>
    </div>
  </div></div>
</div>
</div>
<%- include ('./partials/footer'); %>  
<script src="api/admin/controllers/syncAll.js"></script>
<script src="js/upload.js"></script>
<script src="api/admin/controllers/verifyTPIN.js"></script>  
<script src="api/entityApi/controllers/entityverification.js" type="text/javascript"></script>
<script src="api/entityApi/controllers/kycList.js" type="text/javascript"></script>
</body>
</html>