<!DOCTYPE html>
<html>
  <head>
    <%- include ('./partials/head'); %>
<link rel="stylesheet" href="css/mapInWindow.css">
<link rel="stylesheet" href="css/entity/summary.css">
<link rel="stylesheet" href="css/entity/wizard.css">
</head>
  <body class="skin-green fixed sidebar-mini sidebar-collapse">
    <div class="wrapper">
      <%- include ('./partials/header'); %>
      <div class="content-wrapper">
<div class="row">
    <div class="col-lg-12">
        <div class="box box-widget box-widgets" id="newaddr">
            <div class="box-header">
                <h3 class="box-title"> <i class="fa fa-list"></i> Contact Address
                </h3>
                <!-- <div class="box-tools pull-right">
                <a id="updateProf" class="hidden" type="button" onclick="updateAddr();"
                    title="update profile information" class="btn btn-box-tool"><i
                        class="fa fa-2x fa-floppy-o"></i></a>
                <a type="button" id="btnedit" title="edit profile information" class="btn btn-box-tool"><i
                        class="fa fa-2x fa-pencil"></i></a>
            </div> -->
                <hr>
            </div>
            <div class="box-body">
                <section>
                    <div class="activation wizard">

                        <div class="wizard-inner">
                            <div class="connecting-line"></div>
                            <ul class="nav nav-tabs" role="tablist">
                                <li role="presentation" class="active">
                                    <a href="#step1" data-toggle="tab" aria-controls="step1" role="tab"
                                        title="Company Details">
                                        <span class="round-tab">
                                            <i class="glyphicon glyphicon-folder-open"></i>
                                        </span>
                                    </a>
                                </li>
                                <li role="presentation" class="disabled">
                                    <a href="#step2" data-toggle="tab" aria-controls="step2" role="tab"
                                        title="Comapany Address">
                                        <span class="round-tab">
                                            <i class="glyphicon glyphicon-map-marker"></i>
                                        </span>
                                    </a>
                                </li>
                                <li role="presentation" class="disabled">
                                    <a href="#complete" data-toggle="tab" aria-controls="complete" role="tab"
                                        title="Complete">
                                        <span class="round-tab">
                                            <i class="glyphicon glyphicon-file"></i>
                                        </span>
                                    </a>
                                </li>
                            </ul>
                        </div>

                        <form role="form" data-toggle="validator">
                            <div class="tab-content">
                                <div class="tab-pane active" role="tabpanel" id="step1">
                                    <div class="row">
                                        <div class="col-lg-4">
                                            <div class="form-group">
                                                <!-- <label class="control-label">Contact Address : Remove due to red labelling-->
                                                <label>Building : <span class="text text-danger">*</span>
                                                   
                                                </label>
                                                <div>
                                                    <div class="input-group">
                                                        <span class="input-group-addon">
                                                            <span class="fa fa-home"></span>
                                                        </span>
                                                        <input data-parsley-type="alphanum" id="txtappa" type="text"
                                                            class="form-control" required maxlength="50" value=""
                                                            placeholder="Appartment No / Building No" />
                                                    </div>
                                                    <p class="text-danger" id="error_appa"></p>
                                                </div>
                                            </div>
                                        </div><div class="col-lg-4">
                                            <div class="form-group">
                                                <!-- <label class="control-label">Landmark :</label> Remove due to red labelling -->
                                                <label>Landmark : <span class="text text-danger">*</span></label>
                                                <div>
                                                    <div class="input-group">
                                                        <span class="input-group-addon">
                                                            <span class="fa fa-building"></span>
                                                        </span>
                                                        <input data-parsley-type="alphanum" id="txtlandmark" type="text"
                                                            class="form-control" maxlength="50" value=""
                                                            placeholder="Enter your Landamark" required />
                                                    </div>
                                                    <p class="text-danger" id="error_landmark"></p>
                                                </div>
                                            </div>
                                        </div><div class="col-lg-4">
                                            <div class="form-group">
                                                <!-- <label class="control-label">Street No / Name :</label> Remove due to red labelling-->
                                                <label>Street No / Name : <span
                                                        class="text text-danger">*</span></label>
                                                <div>
                                                    <div class="input-group">
                                                        <span class="input-group-addon">
                                                            <span class="fa fa-road"></span>
                                                        </span>
                                                        <input data-parsley-type="alphanum"
                                                            value="" id="txtstreet" type="text"
                                                            class="form-control" maxlength="50"
                                                            placeholder="Enter your Street No or Name" required />
                                                    </div>
                                                    <p class="text-danger" id="error_street"></p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-4">
                                            <div class="form-group">
                                                <label>Pincode : <span class="text text-danger">*</span></label>
                                                <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <span class="fa fa-map-marker"></span>
                                                    </span>
                                                    <input type="text" value="" id="txtpincode"
                                                        placeholder="Enter Pincode" maxlength="6" required
                                                        class="form-control number">

                                                </div>
                                                <p class="text-danger" id="error_pin"></p>
                                            </div>
                                        </div><div class="col-lg-4">
                                            <div class="form-group">
                                                <label>City : <span class="text text-danger">*</span></label>
                                                <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <span class="fa fa-map"></span>
                                                    </span>
                                                    <input type="text" value="" class="form-control alpha"
                                                        maxlength="50" required id="txtcity"
                                                        placeholder="Enter your city" />
                                                </div>
                                                <p class="text-danger" id="error_city"></p>
                                            </div>
                                        </div><div class="col-lg-4">
                                            <div class="form-group">
                                                <label>State : <span class="text text-danger">*</span></label>
                                                <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <span class="fa fa-globe"></span>
                                                    </span>
                                                    <input type="text" value="" class="form-control alpha"
                                                        maxlength="50" required id="txtstate"
                                                        placeholder="Enter your state" />
                                                </div>
                                                <p class="text-danger" id="error_state"></p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-4">
                                            <div class="form-group">
                                                <label>Country : <span class="text text-danger">*</span></label>
                                                <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <span class="fa fa-flag"></span>
                                                    </span>
                                                    <input type="text" value="" class="form-control alpha"
                                                        maxlength="50" id="txtcountry" placeholder="Enter your Country"
                                                        required />
                                                </div>
                                                <p class="text-danger" id="error_country"></p>
                                            </div>

                                        </div>
                                       
                                    </div>
                                    <ul class="list-inline pull-right">
                                        <li><button type="button" id="submit_step1" class="btn btn-info next-step">Save
                                                and
                                                continue</button></li>
                                    </ul>
                                </div>
                                <div class="tab-pane" role="tabpanel" id="step2">
                                    <div class="col-sm-12">
                                        <div class="container-fuild">
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    <button type="button" class="btn btn-sm btn-info"
                                                        value="Verify Your Location" id="loadprop"><i
                                                            class="fa fa-map-marker">
                                                            Locate Your
                                                            Property</i></button>
                                                    <!-- <button type='button' class='btn btn-sm btn-primary pull-right'
                                                                id='step3' disabled><i class="fa fa-plus">
                                                                    Submit</i></button> -->
                                                </div>
                                                <div class="col-sm-12">
                                                    <div id="map"></div>
                                                    <input type="hidden" id="lat" name="lat">
                                                    <input type="hidden" id="lng" name="lng">
                                                </div>
                                            </div>

                                        </div>
                                        <ul class="list-inline pull-right">
                                            <li><button type="button"
                                                    class="btn btn-default prev-step">Previous</button>
                                            </li>
                                            <li><button type="button" id="submit_step2"
                                                    class="btn btn-info next-step">Save
                                                    and
                                                    continue</button>
                                            </li>
                                        </ul>
                                    </div>
                                </div>

                                <div class="tab-pane" role="tabpanel" id="complete">
                                    <div class="row setup-content" id="step-3">
                                        <div class="col-sm-12">
                                            <div class="container-fuild">
                                                <div class="row moreupload">
                                                    <div class="col-xs-12 col-md-4">
                                                        <div class="form-group">
                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <span class="fa fa-file"></span>
                                                                </span>
                                                                <select type="text" class="form-control subdmitId"
                                                                    required id="subdmitId" name="subdmitId[]"
                                                                    placeholder="">
                                                                    <option>PAN Card</option>
                                                                    <option>Adhar Number</option>
                                                                    <option>Other</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-xs-12 col-md-4">
                                                        <div class="form-group">
                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <span class="fa fa-file-text-o"></span>
                                                                </span>

                                                                <input type="text" name="kycdocnum[]" maxlength="20"
                                                                    class="form-control kycdocnum alphanum" required
                                                                    id="kycdocnum"
                                                                    placeholder="Enter your document number" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-xs-12 col-md-3">
                                                        <div class="form-group">
                                                            <div class="input-group">
                                                                <span class="input-group-addon">
                                                                    <span class="fa fa-upload"></span>
                                                                </span>
                                                                <input id="kycdoc" name="files[]" type="file"
                                                                    class="form-control kycdoc" /></div>
                                                        </div>
                                                    </div>
                                                    <div class='col-xs-12 col-md-1'>
                                                        <div class='form-group' id="kycadd0">
                                                            <a href='javascript:void(0);'
                                                                class='text text-danger fa fa-2x fa-trash btnkycRemove'></a>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="form-group add">
                                                    <a id="btnkycAdd" href="javascript:void(0);"
                                                        class="btn btn-info mdi mdi-plus btn btnkycAdd"
                                                        onclick="addDoc();">Add</a>
                                                    <input type="button" class="btn btn-info mdi mdi-plus"
                                                        id="btnSubmit" value="Submit" />
                                                </div>
                                                <div class="btnup">
                                                    <div id="progress-wrp">
                                                        <div class="progress-bar"></div>
                                                        <div class="status">0%</div>
                                                    </div>
                                                    <div id="output"></div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                    <ul class="list-inline pull-right">
                                        <li><button type="button" class="btn btn-info next-step"
                                                id="submit_step3">Submit</button>
                                        </li>
                                    </ul>
                                </div>
                                <div class="clearfix"></div>
                            </div>
                        </form>

                    </div>

                </section>
            </div>

        </div>
    </div>
</div>
</div> 
    </div>
    <%- include ('./partials/footer'); %>
<script>
    $('.radio-group label').on('click', function () {
        $(this).removeClass('not-active').siblings().addClass('not-active');
    });
</script>
<script src="js/entity/addresses.js"></script>
<script src="js/entity/entityvalidations.js"></script>
<script src="api/entityApi/controllers/saveEntityAddr.js"></script>
<script src="js/entity/entitymap.js"></script>
<script src="js/entity/entityStepWizard.js"></script>
<script src="js/globalVal.js"></script></body>
</html>