<!DOCTYPE html>
<html>
  <head>
    <%- include ('./partials/head'); %>
<link rel="stylesheet" href="truComponents/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css">
<link rel="stylesheet" href="css/entity/summary.css">
<link rel="stylesheet" href="css/entity/wizard.css">
<style>
    .fileview {
        padding: 5px 10px;
        font-size: 1.5em;
        border-radius: 15px;
    }

    .small-box h3 {
        font-size: 24px;
    }

    .shadow .box-footer {
        background: #f4f4f4;
        padding: 1.75rem 2.25rem !important;
        background-color: #f7f7f9;
        border-top: 1px solid rgba(0, 0, 0, .125);
    }

    .small-box .icon {
        top: -5px;
        font-size: 70px;
    }

    .small-box:hover .icon {
        font-size: 75px;
    }
</style>
</head>
  <body class="skin-green fixed sidebar-mini sidebar-collapse">
    <div class="wrapper">
      <%- include ('./partials/header'); %>
      <div class="content-wrapper">
<div class="containerbg">

    <div class="box box-danger">
        <div class="box-header with-border">
            <h3 class="box-title">Register Partner</h3>

            <div class="box-tools pull-right">
                <!--  <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i>
        </button>-->
        <a type="button"  class="btn btn-box-tool hidden" id="edit"  title="Edit Details"><i class="fa fa-pencil"></i></a>
                <a type="button" class="btn btn-box-tool dashboard" data-widget="remove"><i class="fa fa-times"></i></a>

            </div>
        </div>
        <div class="box-body">
            <section>
                <div class="activation wizard">

                    <div class="wizard-inner">
                        <div class="connecting-line"></div>
                        <ul class="nav nav-tabs" role="tablist">
                            <li role="presentation" class="active">
                                <a href="#step1" data-toggle="tab" aria-controls="step1" role="tab"
                                    title="Company Details">
                                    <span class="round-tab">
                                        <i class="glyphicon glyphicon-folder-open"></i>
                                    </span>
                                </a>
                            </li>
                            <li role="presentation" class="">
                                <a href="#step2" data-toggle="tab" aria-controls="step2" role="tab"
                                    title="Comapany Address">
                                    <span class="round-tab">
                                        <i class="glyphicon glyphicon-map-marker"></i>
                                    </span>
                                </a>
                            </li>
                            <li role="presentation" class="">
                                <a href="#complete" data-toggle="tab" aria-controls="complete" role="tab"
                                    title="Document Upload">
                                    <span class="round-tab">
                                        <i class="glyphicon glyphicon-file"></i>
                                    </span>
                                </a>
                            </li>
                        </ul>
                    </div>

                    <form role="form" id="kycForm" data-toggle="validator">
                        <input type="hidden" id="txtcTruid" value="">
                        <div class="tab-content">
                            <div class="tab-pane active" role="tabpanel" id="step1">
                                    
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <label>Company Name</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-building-o"></i></span>
                                                <input type="text" id="txtcname" class="form-control" maxlength="100"
                                                    required placeholder="Company Name" />
                                            </div>
                                            <p class="text-danger" id="error_cname"></p>
                                        </div>
                                    </div>
                                    
                                </div>
                                <div class="row">
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label>First Name</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                                <input type="text" id="txtfname" class="form-control alphanum"
                                                    maxlength="100" required placeholder="First Name" />
                                            </div>
                                            <p class="text-danger" id="error_fname"></p>
                                        </div>
                                    </div>
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label>Middle Name</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                                <input type="text" id="txtmname" class="form-control alphanum"
                                                    maxlength="100" required placeholder="Middle Name" />
                                            </div>
                                            <p class="text-danger" id="error_mname"></p>
                                        </div>
                                    </div>
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label>Last Name</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                                <input type="text" id="txtlname" class="form-control alphanum"
                                                    maxlength="100" required placeholder="Last Name" />
                                            </div>
                                            <p class="text-danger" id="error_lname"></p>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <label>Email-Id</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-envelope"></i></span>
                                                <input type="email" id="txtemail" class="form-control" required
                                                    parsley-type="email" placeholder="Enter a valid e-mail" />
                                            </div>
                                            <p class="text-danger" id="error_email"></p>
                                        </div>
                                    </div>
                                    <div class="col-lg-2">
                                        <div class="form-group">
                                            <label>Country Code :</label>
                                            <div class="input-group">
                                                <div class="input-group-addon">
                                                    <i class="fa fa-flag"></i>
                                                </div>
                                                <!-- <select name="txtCountryCode" id="txtCountryCode" class="form-control selectpicker"
                          data-live-search="true">
                          <option value="+91" selected>+91 (IN)</option>
                          <option value="+1">+1 (US)</option>
                          <option value="+971">+971 (AE)</option>
                          <option value="+880">+880 (BD)</option>
                          <option value="+977">+977 (NP)</option>
                        </select> -->
                                                <input type="text" id="txtCountryCode" class="form-control" required
                                                    placeholder="+91" />
                                            </div>
                                            <div class="text-danger" id="error_CountryCode"></div>
                                        </div>
                                    </div>
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label>Mobile No.</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-phone"></i></span>
                                                <input type="text" id="txtmobileno" class="form-control number"
                                                    maxlength="10" required placeholder="Mobile No." />
                                            </div>
                                            <p class="text-danger" id="error_mobileno"></p>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">


                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <label>Birth Date</label>
                                            <span class="text-danger">*</span>
                                            <div>
                                                <div class="input-group">
                                                    <span class="input-group-addon bg-custom b-0"><i
                                                            class="fa fa-calendar"></i></span>
                                                    <input type="text" class="form-control" id="txtDOB" readonly
                                                        required placeholder="mm/dd/yyyy" id="datepicker">

                                                </div>
                                            </div>
                                            <p class="text-danger" id="error_DOB"></p>
                                        </div>

                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <label>Gender</label>
                                            <span class="text-danger">*</span>
                                            <div class="summarycardfiltre">
                                                <div class="btn-group radio-group">
                                                    <label class="btn btn-info btn-sm active" id="male">
                                                        <input type="radio" name="gender" value="male"
                                                            autocomplete="off" checked>Male
                                                    </label>
                                                    <label class="btn btn-info btn-sm not-active" id="female">
                                                        <input type="radio" name="gender" value="female"
                                                            autocomplete="off">Female
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                                <ul class="list-inline pull-right">
                                    <li><button type="button" id="submit_step1" class="btn btn-warning next-step">
                                            <i class="fa fa-unchecked"></i>Verify and continue</button></li>
                                </ul>
                            </div>
                            <div class="tab-pane" role="tabpanel" id="step2">
                                <div class="col-sm-12">
                                    <div class="row">
                                        <div class="">
                                            <h4>Contact Address
                                                <span class="text-danger">*</span></h4>
                                            <br>
                                            <div class="row">
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label>House Number</label>
                                                        <span class="text-danger">*</span>
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i
                                                                    class="fa fa-home "></i></span>
                                                            <input type="text" id="txtappa" class="form-control"
                                                                required placeholder="Appartment No./ Building No." />
                                                        </div>
                                                        <p class="text-danger" id="error_appa"></p>
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label>Landmark</label>
                                                        <span class="text-danger">*</span>
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i
                                                                    class="fa fa-building"></i></span>
                                                            <input type="text" id="txtlandmark" class="form-control"
                                                                required placeholder="Enter your landmark" />
                                                        </div>
                                                        <p class="text-danger" id="error_landmark"></p>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label>Street No. / Name</label>
                                                        <span class="text-danger">*</span>
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i
                                                                    class="fa fa-road"></i></span>
                                                            <input type="text" id="txtstreet" class="form-control"
                                                                required placeholder="Enter your street no. or name" />
                                                        </div>
                                                        <p class="text-danger" id="error_street"></p>
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label>Pincode</label>
                                                        <span class="text-danger">*</span>
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i
                                                                    class="fa fa-map-marker"></i></span>
                                                            <input type="text" id="txtpincode"
                                                                class="form-control number" maxlength="6" required
                                                                placeholder="Enter pincode" />
                                                        </div>
                                                        <p class="text-danger" id="error_pin"></p>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-4">
                                                    <div class="form-group">
                                                        <label>City</label>
                                                        <span class="text-danger">*</span>
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i
                                                                    class="fa fa-map"></i></span>
                                                            <input type="text" id="txtcity" class="form-control"
                                                                maxlength="50" required placeholder="Enter your city" />
                                                        </div>
                                                        <p class="text-danger" id="error_city"></p>
                                                    </div>
                                                </div>

                                                <div class="col-lg-4">
                                                    <div class="form-group">
                                                        <label>State</label>
                                                        <span class="text-danger">*</span>
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i
                                                                    class="fa fa-map"></i></span>
                                                            <input type="text" class="form-control" id="txtstate"
                                                                maxlength="50" required
                                                                placeholder="Enter your state" />
                                                        </div>
                                                        <p class="text-danger" id="error_state"></p>
                                                    </div>
                                                </div>
                                                <div class="col-lg-4">
                                                    <div class="form-group">
                                                        <label>Country</label>
                                                        <span class="text-danger">*</span>
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i
                                                                    class="fa fa-map"></i></span>
                                                            <input type="text" class="form-control" id="txtcountry"
                                                                maxlength="50" required
                                                                placeholder="Enter your country" />
                                                        </div>
                                                        <p class="text-danger" id="error_country"></p>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                    <ul class="list-inline pull-right">
                                        <!-- <li><button type="button" class="btn btn-default prev-step">Previous</button>
                    </li> -->
                                        <li><button type="button" id="submit_step2" class="btn btn-warning next-step"><i
                                                    class="fa fa-unchecked"></i>Verify
                                                and
                                                continue</button>
                                        </li>
                                    </ul>
                                </div>
                            </div>

                            <div class="tab-pane" role="tabpanel" id="complete">
                                <div class="row setup-content" id="step-3">
                                    <div class="col-sm-12">
                                        <div class="container-fuild">
                                            <div class="row dockychide hidden">
                                                <div class="col-lg-12">
                                                    <p class="text text-info"> Please upload kyc document here... </p>
                                                </div>
                                            </div>
                                            <div class="row moreupload"> </div>
                                            <div class="row dockychide">
                                                <div class="form-group add pull-right">
                                                    <a class="btn btn-primary btn-sm mdi mdi-plus hidden"
                                                        data-toggle="modal" href="#myModals" id="btnadddoc">Add KYC
                                                        Doc</a>
                                                    <input type="submit" disabled class="btn btn-primary btn-sm"
                                                        id="btnSubmit" value="Activate" />
                                                </div>
                                            </div>
                                            <div class="btnup">
                                                <div id="progress-wrp">
                                                    <div class="progress-bar"></div>
                                                    <div class="status">0%</div>
                                                </div>
                                                <div id="output"></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </form>
                </div>
            </section>
            <div class="modal fade" id="myModals" data-backdrop="static" data-keyboard="false">
                <div class="modal-dialog modal-dialogs">
                    <div class="modal-content">

                        <div class="modal-body">
                            <button type="button" class="close text-danger" data-dismiss="modal"><i
                                    class="fa fa-times"></i></button>
                            <br>
                            <hr style="margin-top:10px;">
                            <form method="post" action='' id="login_form">
                                <div class="row fileuploadData">
                                    <div class="col-xs-12 col-sm-12 col-md-12">
                                        <div class="form-group">
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    <span class="fa fa-id-card"></span>
                                                </span>
                                                <select type="text" class="form-control" required id="cmbBox"
                                                    placeholder="">
                                                    <option value="0">Select Document</option>
                                                    <option value="Electricity Bill">Electricity Bill</option>
                                                    <option value="PAN card">PAN card</option>
                                                    <option value="Aadhaar_No">Aadhaar No</option>
                                                    <option>Shop n establishment [Gumasta] Number</option>
                                                    <option>BIS License for HAlMARK</option>
                                                    <option value="VAT Number">VAT Number</option>
                                                    <option>CST Number</option>
                                                    <option>GST Number</option>
                                                    <option>Property Ownership</option>
                                                    <option>Insurance</option>
                                                    <option>Other</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-sm-12 col-md-12">
                                        <div class="form-group">
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    <span class="fa fa-file"></span>
                                                </span>
                                                <input type="text" maxlength="20" class="form-control alphanum" required
                                                    id="docNumber" placeholder="Enter your document number" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-12 col-md-12 filez">
                                        <div class="form-group">
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    <span class="fa fa-upload"></span>
                                                </span>

                                                <input type='file' id='kycdocFile1' name='files'
                                                    class='form-control kycdoc' accept=".jpg, .jpeg, .pdf,.doc,.docx" /></div>

                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <span class="pdoc text text-danger"></span>
                                    </div>
                                </div>
                            </form>
                        </div>
                        <div class="modal-footer">
                            <a id="btnkycAdd" href="javascript:void(0);"
                                class="btn btn-sm btn-primary mdi mdi-plus btn btnkycAdd">Add</a>
                            <button type="button" class="btn btn-danger btn-sm" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        </form>

    </div>
</div>
</section>
</div>
</div></div> 
    </div>
