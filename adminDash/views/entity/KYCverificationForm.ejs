<!DOCTYPE html>
<html>
  <head>
    <%- include ('./partials/head'); %>
<link rel="stylesheet" href="css/wizard.css">
<link rel="stylesheet" href="truComponents/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css">
<link rel="stylesheet" href="css/entity/summary.css">
<link rel="stylesheet" href="css/mapInWindow.css">
</head>
  <body class="skin-green fixed sidebar-mini sidebar-collapse">
    <div class="wrapper">
      <%- include ('./partials/header'); %>
      <div class="content-wrapper">


<div class="containerbg">

    <div class="box box-info">
        <div class="box-header with-border">
            <h3 class="box-title">Consumer KYC Verification</h3>

            <div class="box-tools pull-right">
                <!--  <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i>
        </button>-->
                <a type="button" class="btn btn-box-tool dashboard" data-widget="remove"><i class="fa fa-times"></i></a>

            </div>
        </div>

        <div class="box-body">
            <section>
                <div class="activation wizard">

                    <div class="wizard-inner">
                        <div class="connecting-line"></div>
                        <ul class="nav nav-tabs" role="tablist">
                            <li role="presentation" class="active">
                                <a href="#step1" data-toggle="tab" aria-controls="step1" role="tab"
                                    title="Company Details">
                                    <span class="round-tab">
                                        <i class="glyphicon glyphicon-folder-open"></i>
                                    </span>
                                </a>
                            </li>
                            <li role="presentation" class="disabled">
                                <a href="#step2" data-toggle="tab" aria-controls="step2" role="tab"
                                    title="Comapany Address">
                                    <span class="round-tab">
                                        <i class="glyphicon glyphicon-map-marker"></i>
                                    </span>
                                </a>
                            </li>
                            <li role="presentation" class="disabled">
                                <a href="#complete" data-toggle="tab" aria-controls="complete" role="tab"
                                    title="Complete">
                                    <span class="round-tab">
                                        <i class="glyphicon glyphicon-file"></i>
                                    </span>
                                </a>
                            </li>
                        </ul>
                    </div>

                    <form role="form" data-toggle="validator">
                        <div class="tab-content">
                            <div class="tab-pane active" role="tabpanel" id="step1">
                                <div class="row">
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label>First Name</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                                <input type="text" id="txtfname" class="form-control alphanum"
                                                    maxlength="100" required placeholder="First Name" />
                                            </div>
                                            <p class="text-danger" id="error_fname"></p>
                                        </div>
                                    </div>
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label>Middle Name</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                                <input type="text" id="txtmname" class="form-control alphanum"
                                                    maxlength="100" required placeholder="Middle Name" />
                                            </div>
                                            <p class="text-danger" id="error_mname"></p>
                                        </div>
                                    </div>
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label>Last Name</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                                <input type="text" id="txtlname" class="form-control alphanum"
                                                    maxlength="100" required placeholder="Last Name" />
                                            </div>
                                            <p class="text-danger" id="error_lname"></p>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <label>Email-Id</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-envelope"></i></span>
                                                <input type="email" id="txtemail" class="form-control" required
                                                    parsley-type="email" placeholder="Enter a valid e-mail" />
                                            </div>
                                            <p class="text-danger" id="error_email"></p>
                                        </div>
                                    </div>
                                    <div class="col-lg-2">
                                        <div class="form-group">
                                            <label>Country Code</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-phone"></i></span>
                                                <input type="text" id="txtccode" class="form-control ccode"
                                                    maxlength="5" required placeholder="+91" />
                                            </div>
                                            <p class="text-danger" id="error_ccode"></p>
                                        </div>
                                    </div>
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label>Mobile No.</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-phone"></i></span>
                                                <input type="text" id="txtmobileno" class="form-control number"
                                                    maxlength="10" required placeholder="Mobile No." />
                                            </div>
                                            <p class="text-danger" id="error_mobileno"></p>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">

                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <label>Birth Date</label>
                                            <span class="text-danger">*</span>
                                            <div>
                                                <div class="input-group">
                                                    <span class="input-group-addon bg-custom b-0"><i
                                                            class="fa fa-calendar"></i></span>
                                                    <input type="text" class="form-control getdob" id="txtDOB" required
                                                        placeholder="mm/dd/yyyy" id="datepicker">

                                                </div><!-- input-group -->
                                            </div>
                                            <p class="text-danger" id="error_DOB"></p>
                                        </div>

                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <label>Gender</label>
                                            <span class="text-danger">*</span>
                                            <div class="summarycardfiltre">
                                                <div class="btn-group radio-group" data-toggle="buttons">
                                                    <label class="btn btn-info btn-sm active" id="buy_func">
                                                        <input type="radio" name="options" autocomplete="off"
                                                            checked>Male
                                                    </label>
                                                    <label class="btn btn-info btn-sm not-active" id="redeem_func">
                                                        <input type="radio" name="options" autocomplete="off">Female
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <hr>
                                <div class="row">
                                    <div class="col-lg-6">
                                        <label>Billing Address</label>
                                        <span class="text-danger">*</span>
                                        <small>
                                            <div class="input-group">
                                                <div class="btn-group radio-group">
                                                    <label class="btn-sm not-active1">|</label>
                                                    <br>
                                                </div>
                                            </div>
                                        </small>
                                        <div class="form-group">
                                            <label>House Number</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-home "></i></span>
                                                <input type="text" id="appa" class="form-control" required
                                                    placeholder="Appartment No./ Building No." />
                                            </div>
                                            <p class="text-danger" id="error_appa"></p>
                                        </div>
                                        <div class="form-group">
                                            <label>Landmark</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-building"></i></span>
                                                <input type="text" id="txtlandmark" class="form-control" required
                                                    placeholder="Enter your landmark" />
                                            </div>
                                            <p class="text-danger" id="error_landmark"></p>
                                        </div>
                                        <div class="form-group">
                                            <label>Street No. / Name</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-road"></i></span>
                                                <input type="text" id="txtstreet" class="form-control" required
                                                    placeholder="Enter your street no. or name" />
                                            </div>
                                            <p class="text-danger" id="error_street"></p>
                                        </div>
                                        <div class="form-group">
                                            <label>Pincode</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-map-marker"></i></span>
                                                <input type="text" id="txtpincode" class="form-control number"
                                                    maxlength="6" required placeholder="Enter pincode" />
                                            </div>
                                            <p class="text-danger" id="error_pin"></p>
                                        </div>
                                        <div class="form-group">
                                            <label>City</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-map"></i></span>
                                                <input type="text" id="txtcity" class="form-control" maxlength="50"
                                                    required placeholder="Enter your city" />
                                            </div>
                                            <p class="text-danger" id="error_city"></p>
                                        </div>
                                        <div class="form-group">
                                            <label>State</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-map"></i></span>
                                                <input type="text" class="form-control" id="txtstate" maxlength="50"
                                                    required placeholder="Enter your state" />
                                            </div>
                                            <p class="text-danger" id="error_state"></p>
                                        </div>
                                        <div class="form-group">
                                            <label>Country</label>
                                            <span class="text-danger">*</span>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-map"></i></span>
                                                <input type="text" class="form-control" id="txtcountry" maxlength="50"
                                                    required placeholder="Enter your country" />
                                            </div>
                                            <p class="text-danger" id="error_country"></p>
                                        </div>
                                    </div>

                                    <div class="col-lg-6">
                                        <label>Permanant Address</label>

                                        <div class="form-group" style="margin-bottom: 7px;">
                                            <div class="summarycardfiltre">
                                                <div class="btn-group radio-group">
                                                    <label class="btn btn-info btn-sm not-active">Same
                                                        as my contact
                                                        address <input type="radio" value="True" name="reg"></label>
                                                    <label class="btn btn-info btn-sm active">Not
                                                        same <input type="radio" value="False" name="reg"></label>


                                                </div>
                                            </div>

                                        </div>
                                        <div class="form-group">
                                            <label>House Number</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-home "></i></span>
                                                <input type="text" id="b_appa" class="form-control" required
                                                    placeholder="Appartment No./ Building No." />
                                            </div>
                                            <p class="text-danger" id="error_b_appa"></p>
                                        </div>
                                        <div class="form-group">
                                            <label>Landmark</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-building"></i></span>
                                                <input type="text" id="b_txtlandmark" class="form-control" required
                                                    placeholder="Enter your landmark" />
                                            </div>
                                            <p class="text-danger" id="error_b_landmark"></p>
                                        </div>
                                        <div class="form-group">
                                            <label>Street No. / Name</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-road"></i></span>
                                                <input type="text" id="b_txtstreet" class="form-control" required
                                                    placeholder="Enter your street no. or name" />
                                            </div>
                                            <p class="text-danger" id="error_b_street"></p>
                                        </div>
                                        <div class="form-group">
                                            <label>Pincode</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-map-marker"></i></span>
                                                <input type="text" id="b_txtpincode" class="form-control number"
                                                    maxlength="6" required placeholder="Enter pincode" />
                                            </div>
                                            <p class="text-danger" id="error_b_pin"></p>
                                        </div>
                                        <div class="form-group">
                                            <label>City</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-map"></i></span>
                                                <input type="text" id="b_txtcity" class="form-control" maxlength="50"
                                                    required placeholder="Enter your city" />
                                            </div>
                                            <p class="text-danger" id="error_b_city"></p>
                                        </div>
                                        <div class="form-group">
                                            <label>State</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-map"></i></span>
                                                <input type="text" class="form-control" id="b_txtstate" maxlength="50"
                                                    required placeholder="Enter your state" />
                                            </div>
                                            <p class="text-danger" id="error_b_state"></p>
                                        </div>
                                        <div class="form-group">
                                            <label>Country</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-map"></i></span>
                                                <input type="text" class="form-control" id="b_txtcountry" maxlength="50"
                                                    required placeholder="Enter your country" />
                                            </div>
                                            <p class="text-danger" id="error_b_country"></p>
                                        </div>
                                    </div>
                                </div>
                                <ul class="list-inline pull-right">
                                    <li><button type="button" id="submit_step1" class="btn btn-info next-step">Save
                                            and
                                            continue</button></li>
                                </ul>
                            </div>
                            <div class="tab-pane" role="tabpanel" id="step2">
                                <div class="col-sm-12">
                                    <div class="container-fuild">
                                        <div class="row">
                                            <div class="col-sm-12">
                                                <button type="button" class="btn btn-sm btn-info"
                                                    value="Verify Your Location" id="loadprop"><i
                                                        class="fa fa-map-marker">
                                                        Locate Your
                                                        Property</i></button>
                                                <!-- <button type='button' class='btn btn-sm btn-primary pull-right'
                                                                id='step3' disabled><i class="fa fa-plus">
                                                                    Submit</i></button> -->
                                            </div>
                                            <div class="col-sm-12">
                                                <div id="map"></div>
                                                <input type="hidden" id="lat" name="lat">
                                                <input type="hidden" id="lng" name="lng">
                                            </div>
                                        </div>

                                    </div>
                                    <ul class="list-inline pull-right">
                                        <li><button type="button" class="btn btn-default prev-step">Previous</button>
                                        </li>
                                        <li><button type="button" id="submit_step2" class="btn btn-info next-step">Save
                                                and
                                                continue</button>
                                        </li>
                                    </ul>
                                </div>
                            </div>

                            <div class="tab-pane" role="tabpanel" id="complete">
                                <div class="row setup-content" id="step-3">
                                    <div class="col-sm-12">
                                        <div class="container-fuild">
                                            <div class="row moreupload">
                                                <div class="col-xs-12 col-sm-6 col-md-4">
                                                    <div class="form-group">
                                                        <div class="input-group">
                                                            <span class="input-group-addon">
                                                                <span class="fa fa-send"></span>
                                                            </span>

                                                            <select type="text" class="form-control subdmitId" required
                                                                id="subdmitId" name="subdmitId[]" placeholder="">
                                                                <option value="Electricity Bill">
                                                                    Electricity Bill
                                                                </option>
                                                                <option value="PAN card">PAN
                                                                    card</option>
                                                                <option>Shop n establishment
                                                                    [Gumasta] Number
                                                                </option>
                                                                <option>BIS License for
                                                                    HAlMARK</option>
                                                                <option value="VAT Number">
                                                                    VAT Number</option>
                                                                <option>CST Number</option>
                                                                <option>GST Number</option>
                                                                <option>Property Ownership
                                                                </option>
                                                                <option>Insurance</option>
                                                                <option>Other</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-6 col-md-4">
                                                    <div class="form-group">
                                                        <div class="input-group">
                                                            <span class="input-group-addon">
                                                                <span class="fa fa-file"></span>
                                                            </span>

                                                            <input type="text" name="kycdocnum[]" maxlength="20"
                                                                class="form-control kycdocnum alphanum" required
                                                                id="kycdocnum"
                                                                placeholder="Enter your document number" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-6 col-md-3">
                                                    <div class="form-group">
                                                        <div class="input-group">
                                                            <span class="input-group-addon">
                                                                <span class="fa fa-file-photo-o (alias)"></span>
                                                            </span>
                                                            <input id="kycdoc" name="file[]" type="file"
                                                                class="form-control kycdoc" />
                                                        </div>
                                                    </div>
                                                </div>




                                            </div>

                                            <div class="form-group add">
                                                <a id="btnkycAdd" href="javascript:void(0);"
                                                    class="btn btn-info mdi mdi-plus btn btnkycAdd">Add</a>
                                                <input type="button" class="btn btn-info mdi mdi-plus" id="btnSubmit"
                                                    value="Submit" />
                                            </div>
                                            <div class="btnup">
                                                <div id="progress-wrp">
                                                    <div class="progress-bar"></div>
                                                    <div class="status">0%</div>
                                                </div>
                                                <div id="output"></div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <ul class="list-inline pull-right">
                                    <li><button type="button" class="btn btn-info">Submit</button>
                                    </li>
                                </ul>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </form>

                </div>
        </div>
        </section>
    </div>
</div></div> 
    </div>
    <%- include ('./partials/footer'); %>
<!-- Date time picker -->
<script src="truComponents/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js"></script>
<script>
    //Date picker
    $('#txtDOB').datepicker({
        autoclose: true,
        format: 'dd/mm/yyyy'
    })

    // radio button
    $('.radio-group label').on('click', function () {
        $(this).removeClass('not-active').siblings().addClass('not-active');
    });
</script>

<script src="js/companymap.js"></script>
<script src="js/globalVal.js"></script>
<script src="api/entityApi/js/entityKYCvalidations.js"></script></body>
